{
  "cases": [
    {
      "description": "an empty board has no winner",
      "board": [
        ". . . . .",
        " . . . . .",
        "  . . . . .",
        "   . . . . .",
        "    . . . . ."
      ],
      "expected": ""
    },
    {
      "description": "only edges does not make a winner",
      "board": [
        ". X X X X",
        " . . . . X",
        "  . O . X .",
        "   . O O . .",
        "    . . O O ."
      ],
      "expected": ""
    },
    {
      "description": "illegal diagonal nobody wins",
      "board": [
         "X O . .",
         " O X X X",
         "  O X O .",
         "   . O X .",
         "    X X O O"
      ],
      "expected": ""
    },
    {
      "description": "Rectangle black - black wins",
      "board": [
        ". O . .",
        " O X X X",
        "  O X O .",
        "   X X O X",
        "    . O X ."
      ],
      "expected": "X"
    },
    {
      "description": "Rectangle white - white wins",
      "board": [
        ". O . .",
        " O X X X",
        "  O O O .",
        "   X X O X",
        "    . O X ."
      ],
      "expected": "O"
    },
    {
      "description": "Convoluted path black wins",
      "board": [
        ". X X . .",
        " X . X . X",
        "  . X . X .",
        "   . X X . .",
        "    O O O O O"
      ], 
      "expected": "X"
    },
    {
      "description": "spiral black wins",
      "board": [
        "O X X X X X X X X",
        " O X O O O O O O O",
        "  O X O X X X X X O",
        "   O X O X O O O X O",
        "    O X O X X X O X O",
        "     O X O O O X O X O",
        "      O X X X X X O X O",
        "       O O O O O O O X O",
        "        X X X X X X X X O"
      ],
      "expected": "X"
    }
  ]
}